{
  "scripts": {
    "test:run": "tsc && node ./build/src/server/server.js",
    "app": "concurrently -n server,client \"npm run server\" \"npm run client\"",
    "client": "webpack-dev-server --config ./scripts/webpack/webpack.client.js",
    "server": "webpack --config ./scripts/webpack/webpack.server.js",
    "server:watch": "nodemon ./build/server/server.js",
    "build:app": "concurrently -n server,client \"npm run build:server\" \"npm run build:client\"",
    "build:client": "NODE_ENV=production webpack --mode production --config scripts/webpack/webpack.client.js",
    "build:server": "NODE_ENV=production webpack --mode production --config scripts/webpack/webpack.server.js",
    "db:create": "node scripts/db/createDB.js",
    "db:drop": "node scripts/db/dropDB.js",
    "db:migrate": "node scripts/db/migrateDB.js",
    "db:migrate:make": "node scripts/migrations/migrateMake.js",
    "db:seed": "node scripts/db/seedDB.js",
    "db:seed:make": "node scripts/migrations/seedMake.js",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "lint": "npm run lint:client && npm run lint:server",
    "lint:client": "eslint ./src/client/ --color --quiet",
    "lint:server": "eslint ./src/server/ --color --quiet"
  },
